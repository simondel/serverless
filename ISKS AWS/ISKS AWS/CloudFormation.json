{
  "Resources": {
    "RestApi": {
      "Type": "AWS::ApiGateway::RestApi",
      "Properties": {
        "Description": "Created by CloudFormation",
        "EndpointConfiguration": {
          "Types": [ "REGIONAL" ]
        },
        "Name": "ISKS RestApi"
      }
    },
    "RestApiResource": {
      "Type": "AWS::ApiGateway::Resource",
      "Properties": {
        "RestApiId": { "Ref": "RestApi" },
        "ParentId": { "Fn::GetAtt": [ "RestApi", "RootResourceId" ] },
        "PathPart": "isks"
      }
    }
  }
}
